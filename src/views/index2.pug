        div
          ul.mt-6.leading-10.px-4
            li.mb-3.p-2.rounded-md.flex.items-center.justify-center.bg-blue-400.cursor-pointer(@click='openMenu !== 1 ? openMenu = 1 : openMenu = null')
              i.fas.fa-align-left.fa-sm.text-white
            li.mb-3.p-2.rounded-md.flex.items-center.justify-center.bg-pink-400.cursor-pointer
              i.fas.fa-question-circle.fa-sm.text-white
            li.mb-3.p-2.rounded-md.flex.items-center.justify-center.bg-yellow-400.cursor-pointer
              i.fas.fa-headphones.fa-sm.text-white
            li.absolute.bottom-0.mb-3.p-2.rounded-full.flex.items-center.mx-auto.bg-white.cursor-pointer
              i.fas.fa-power-off.fa-sm.text-indigo-600
    //- 애니메이션 메뉴
    aside.animate__animated.animate__fadeInLeft.w-52.relative.z-0.flex-shrink-0.hidden.px-4.overflow-y-auto.bg-gray-100(class='sm:block' x-show='openMenu ==  1' @click.away='openMenu = null' style='display: none;')
