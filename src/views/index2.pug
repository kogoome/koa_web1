        div.grid.grid-cols-1.gap-4.grid-cols-2.mt-6
          div.p-2.flex.flex-col.items-center.bg-white.rounded-md.justify-center.shadow-xl.cursor-pointer
            div.rounded-full.p-2.bg-indigo-200.flex.flex-col.items-center
              i.fas.fa-chart-pie.fa-sm.text-indigo-600
            p.text-xs.mt-1.text-center.font-semibold Dashboard
          div.p-2.flex.flex-col.items-center.bg-white.rounded-md.justify-center.shadow-xl.cursor-pointer
            div.rounded-full.p-2.bg-indigo-200.flex.flex-col.items-center
              i.fas.fa-calculator.fa-sm.text-indigo-600
            p.text-xs.mt-1.text-center.font-semibold Calculator
          div.p-2.flex.flex-col.items-center.bg-white.rounded-md.justify-center.shadow-xl.cursor-pointer
            div.rounded-full.p-2.bg-indigo-200.flex.flex-col.items-center
              i.fas.fa-wallet.fa-sm.text-indigo-600
