body
  //-사이드메뉴
  div.flex.h-screen.w-full.bg-gray-800(x-data='{openMenu:1}')
    aside.w-20.relative.z-20.flex-shrink-0.px-2.overflow-y-auto.bg-indigo-600(class='sm:block')
      div.mb-6
        div.flex.justify-center
          div.w-14.h-14.rounded-full.bg-gray-300.border-2.border-white.mt-2
