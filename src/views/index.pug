      a(href="/acount/kogoome") acount/kogoome link
    div
      a(href="/post") post link
    div
      a(href="/signin") signin link
    div
      input#input_id(type="text" placeholder="프로필아이디")
      dutton#id(onclick="inputName()") @id
  script(src="/public/js/index.js")